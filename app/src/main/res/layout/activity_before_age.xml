<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".BeforeAge"
    android:orientation="vertical">

    <include
        layout="@layout/toolbar"/>

    <LinearLayout
        android:id="@+id/e1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Plie squat scoop up"
                android:textSize="20dp"
                android:textStyle="bold"
                android:paddingTop="20dp"
                android:paddingLeft="20dp"/>

        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Repeat 2 Time"
                android:textSize="18dp"
                android:paddingTop="10dp"
                android:paddingLeft="20dp"/>

            <ImageView
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:src="@drawable/e1"
                android:layout_marginLeft="100dp"/>

        </LinearLayout>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="01:00 MIN"
            android:textSize="18dp"
            android:paddingTop="10dp"
            android:paddingLeft="20dp"
            android:layout_marginTop="-60dp"
            android:textColor="#EB3324"/>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="________________________________________________________"
            android:layout_marginTop="10dp"/>

    </LinearLayout>

    <LinearLayout
        android:id="@+id/e2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Side lunge front raise"
                android:textSize="20dp"
                android:textStyle="bold"
                android:paddingLeft="20dp"/>

        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Repeat 2 Time"
                android:textSize="18dp"
                android:paddingTop="10dp"
                android:paddingLeft="20dp"/>

            <ImageView
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:src="@drawable/e2"
                android:layout_marginLeft="100dp"/>

        </LinearLayout>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="01:00 MIN"
            android:textSize="18dp"
            android:paddingTop="10dp"
            android:paddingLeft="20dp"
            android:layout_marginTop="-60dp"
            android:textColor="#EB3324"/>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="________________________________________________________"
            android:layout_marginTop="10dp"/>

    </LinearLayout>

    <LinearLayout
        android:id="@+id/e3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Deadlift upright row"
                android:textSize="20dp"
                android:textStyle="bold"
                android:paddingLeft="20dp"/>

        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Repeat 2 Time"
                android:textSize="18dp"
                android:paddingTop="10dp"
                android:paddingLeft="20dp"/>

            <ImageView
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:src="@drawable/e3"
                android:layout_marginLeft="100dp"/>

        </LinearLayout>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="01:00 MIN"
            android:textSize="18dp"
            android:paddingTop="10dp"
            android:paddingLeft="20dp"
            android:layout_marginTop="-60dp"
            android:textColor="#EB3324"/>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="________________________________________________________"
            android:layout_marginTop="10dp"/>

    </LinearLayout>

    <LinearLayout
        android:id="@+id/e4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Ankle tap push ups"
                android:textSize="20dp"
                android:textStyle="bold"
                android:paddingLeft="20dp"/>

        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Repeat 2 Time"
                android:textSize="18dp"
                android:paddingTop="10dp"
                android:paddingLeft="20dp"/>

            <ImageView
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:src="@drawable/e4"
                android:layout_marginLeft="100dp"
                android:layout_marginTop="-10dp"/>

        </LinearLayout>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="01:00 MIN"
            android:textSize="18dp"
            android:paddingTop="10dp"
            android:paddingLeft="20dp"
            android:layout_marginTop="-60dp"
            android:textColor="#EB3324"/>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="________________________________________________________"
            android:layout_marginTop="10dp"/>

    </LinearLayout>


</LinearLayout>